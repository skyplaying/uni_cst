.uni-list:after,
.uni-list:before {
	height: 0;
}
.addr {
	color: #BBBBBB;
}
.shopType {
	color: #848484;
}
.content {
	text-align: center;
	height: 400rpx;
}
.logo {
	height: 200rpx;
	width: 200rpx;
	margin-top: 200rpx;
}
.title {
	font-size: 36rpx;
	color: #8f8f94;
}
.listFont {
	font-size: 32rpx;
	color: black;
}
.shop-distance {
	display: inline-block;
	color: #888888;
	position: absolute;
	right: 30rpx;
}
.shopBox {
	background-color: white;
	padding: 30rpx;
	width: 100%;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-sizing: border-box;
	        box-sizing: border-box;
}
.shopImage {
	width: 150rpx;
	height: 140rpx;
	border-radius: 10rpx;
	overflow: hidden;
}
.shopInfo {
	margin-left: 30rpx;
}

